package aaa;

public class Pattern {

	public static void main(String[] args) {
		
		for(int i=0; i<=5; i++)
		{
			for(int j=i; j<5; j++)
			{
				System.out.print("*");
				if( i==5&&j==0) 
				{
					for(int a=0; a<=5; a++) 
					{
						for(int b=0; b<a; b++)
						{
							System.out.print("*");
						}
						System.out.println("");
					}
				}
			}
			System.out.println("");
			
			
		}

	}

}
