package aaa;

public class Pattern {

	public static void main(String[] args) {
		
		for(int i=0; i<=5; i++)
		{
			for(int j=i; j<5; j++)
			{
				System.out.print("*");
				
			}
			System.out.println("");
			while( i==5) 
			{
				for(int a=0; a<=5; a++) 
				{
					for(int b=0; b<a; b++)
					{
						System.out.print("*");
					}
					System.out.println("");
				}
			}
			
		}

	}

}
